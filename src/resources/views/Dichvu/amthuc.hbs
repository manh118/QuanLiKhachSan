                            <div class="serive__dish__title">{{this.name}}</div>
                            <div class="serive__dish__des">{{this.description}}</div>
                            <div class="serive__dish__price">{{money this.price}} VNĐ</div>
                        </div>
                    </div>
                {{/each}}
            </div>
        </div>
        <div class="tab-pane fade" id="nav-pizza" role="tabpanel" aria-labelledby="nav-pizza-tab">
             <div class="row">
               {{#each DishesByCategory.PIZZA}}
                    <div class="col-6 serive__dish__form">
                        <div class="serive__dish__img">
                            <img src="{{this.img}}" alt="">
                        </div>
                        <div class="serive__dish__content">
                            <div class="serive__dish__title">{{this.name}}</div>
                            <div class="serive__dish__des">{{this.description}}</div>
                            <div class="serive__dish__price">{{money this.price}} VNĐ</div>
                        </div>
                    </div>
                {{/each}}
            </div>
        </div>
        <div class="tab-pane fade" id="nav-salad" role="tabpanel" aria-labelledby="nav-salad-tab">
             <div class="row">
               {{#each DishesByCategory.SALAD}}
                    <div class="col-6 serive__dish__form">
                        <div class="serive__dish__img">
                            <img src="{{this.img}}" alt="">
                        </div>
                        <div class="serive__dish__content">
                            <div class="serive__dish__title">{{this.name}}</div>
                            <div class="serive__dish__des">{{this.description}}</div>
                            <div class="serive__dish__price">{{money this.price}} VNĐ</div>
                        </div>
                    </div>
                {{/each}}
            </div>
        </div>
